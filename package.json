{
  "name": "@vroskus/library-sentry",
  "version": "1.0.7",
  "description": "Sentry",
  "author": "Vilius Ro≈°kus <vilius@regattas.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vroskus/library-sentry.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "npm run test:eslint && npm run test:e2e",
    "test:eslint": "eslint src test --fix",
    "test:e2e": "echo 'No tests'"
  },
  "dependencies": {
    "@sentry/browser": "7.84.0",
    "@sentry/node": "7.84.0",
    "@vroskus/library-error": "1.0.12",
    "lodash": "4.17.21",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.10",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.2",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.55.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-import-newlines": "1.3.4",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-sort": "2.11.0",
    "jest": "29.7.0",
    "jest-coverage-thresholds-bumper": "1.1.0",
    "jest-express": "1.12.0",
    "npm-check": "6.0.1",
    "ts-jest": "29.1.1",
    "typescript": "5.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
