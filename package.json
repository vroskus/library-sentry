{
  "name": "@vroskus/library-sentry",
  "version": "1.0.19",
  "description": "Sentry",
  "author": "Vilius Ro≈°kus <vilius@regattas.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vroskus/library-sentry.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run test:eslint && npm run test:e2e",
    "test:eslint": "eslint src test --fix",
    "test:e2e": "echo 'No tests'"
  },
  "dependencies": {
    "@sentry/browser": "^7.105.0",
    "@sentry/node": "^7.105.0",
    "@sentry/tracing": "^7.105.0",
    "@vroskus/library-error": "^1.0.20",
    "lodash": "4.17.21",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/lodash": "4.14.202",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-problems": "8.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-import-newlines": "1.3.4",
    "eslint-plugin-perfectionist": "^2.6.0",
    "eslint-plugin-react": "^7.34.0",
    "jest": "29.7.0",
    "jest-coverage-thresholds-bumper": "1.1.0",
    "jest-express": "1.12.0",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
