{
  "name": "@vroskus/library-sentry",
  "version": "1.0.2",
  "description": "Sentry",
  "author": "Vilius Ro≈°kus <vilius@regattas.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vroskus/library-sentry.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc",
    "test": "npm run test:eslint && npm run test:e2e",
    "test:eslint": "eslint src test --fix",
    "test:e2e": "echo 'No tests'"
  },
  "dependencies": {
    "@sentry/browser": "7.38.0",
    "@sentry/node": "7.38.0",
    "@types/lodash": "4.14.192",
    "@vroskus/library-error": "1.0.2",
    "lodash": "4.17.21",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-import-newlines": "1.3.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-sort": "2.4.0",
    "jest": "29.4.3",
    "jest-coverage-thresholds-bumper": "1.1.0",
    "jest-express": "1.12.0",
    "npm-check": "6.0.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
